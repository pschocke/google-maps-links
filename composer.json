{
    "name": "pschocke/google-maps-links",
    "description": "easily create google maps links to link to a map",
    "keywords": [
        "Google Maps",
        "Google Maps links"
    ],
    "homepage": "https://github.com/pschocke/google-maps-links",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Schocke",
            "email": "patrick@ausbildung-ms.de",
            "homepage": "https://github.com/pschocke",
            "role": "Maintainer"
        }
    ],
    "require": {},
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "pschocke\\GoogleMapsLinks\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pschocke\\GoogleMapsLinks\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
